{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/**/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/subscriptions.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/payments/(.*)",
      "dest": "/api/payments"
    },
    {
      "src": "/api/products/(.*)",
      "dest": "/api/products"
    },
    {
      "src": "/api/secours/(.*)",
      "dest": "/api/secours"
    },
    {
      "src": "/api/subscriptions/(.*)",
      "dest": "/api/subscriptions"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
