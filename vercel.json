{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/payments/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/payments/initiate-orange-money.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/payments/initiate-sama-money.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/payments/orange-money-webhook.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/payments/cinetpay-webhook.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/payments/verify.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/products/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/secours/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/secours/subscriptions.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/secours/transactions.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/secours/requests.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/subscriptions/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/subscriptions/activate.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/subscriptions/update.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/payments/(.*)",
      "dest": "/api/payments"
    },
    {
      "src": "/api/products/(.*)",
      "dest": "/api/products"
    },
    {
      "src": "/api/secours/(.*)",
      "dest": "/api/secours"
    },
    {
      "src": "/api/subscriptions/(.*)",
      "dest": "/api/subscriptions"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
