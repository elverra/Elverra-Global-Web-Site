{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/payments/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/products/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/secours/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/subscriptions/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/payments/(.*)",
      "dest": "/api/payments/index.js"
    },
    {
      "src": "/api/products/(.*)",
      "dest": "/api/products/index.js"
    },
    {
      "src": "/api/secours/(.*)",
      "dest": "/api/secours/index.js"
    },
    {
      "src": "/api/subscriptions/(.*)",
      "dest": "/api/subscriptions/index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/lovable-uploads/(.*)",
      "dest": "/lovable-uploads/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
